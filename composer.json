{
    "name": "sudoku",
    "type": "project",
    "license": "private",
    "description": "Sudoku solving",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/orm": "^2.6",
        "graylog2/gelf-php": "^1.6",
        "logit/domain-extra-library": "dev-master",
        "ramsey/uuid": "^3.8",
        "ramsey/uuid-doctrine": "^1.5",
        "symfony/console": "^4.2",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.2",
        "symfony/messenger": "^4.2",
        "symfony/monolog-bundle": "^3.3",
        "symfony/serializer-pack": "^1.0",
        "symfony/validator": "^4.2",
        "symfony/yaml": "^4.2",
        "wakeonweb/errors-extra-library": "^1.0",
        "wakeonweb/gelf-extra-logger-bundle": "^1.0"
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "behat/symfony2-extension": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "escapestudios/symfony2-coding-standard": "~3.0",
        "guzzlehttp/psr7": "^1.5",
        "php-http/curl-client": "^1.7",
        "slevomat/coding-standard": "~4.0",
        "squizlabs/php_codesniffer": "~3.0",
        "symfony/test-pack": "^1.0",
        "ubirak/rest-api-behat-extension": "^7.1.0",
        "wakeonweb/behat-contexts": "^1.2",
        "doctrine/data-fixtures": "^1.3",
        "theofidry/alice-data-fixtures": "^1.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "repositories": [
        { "type": "vcs", "url": "https://github.com/dispeo/logit-domain-extra-library.git" }
    ],
    "autoload": {
        "psr-4": {
            "Sudoku\\": "src/Sudoku/"
        },
        "classmap": [ "src/Kernel.php" ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sudoku\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
